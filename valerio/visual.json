    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "storage.modifier.c",
                    "storage.type.built-in.primitive.c"
                ],
                "settings": {
                    "foreground": "#a4fa53"
                }
            },
            {
                "scope": [
                    "string.quoted.double.c"
                ],
                "settings": {
                    "foreground": "#ff8ffd"
                }
            },
            {
                "scope": [
                    "entity.name.function.preprocessor.c"
                ],
                "settings": {
                    "foreground": "#ff8ffd"
                }
            },
            {
                "scope": [
                    "entity.name.function.c"
                ],
                "settings": {
                    "foreground": "#c7c7c7"
                }
            },
            {
                "scope": [
                    "keyword.control.c"
                ],
                "settings": {
                    "foreground": "#f7f582"
                }
            },
            {
                "scope": [
                    "keyword.control.directive.include.c",
                    "meta.preprocessor.include.c"
                ],
                "settings": {
                    "foreground": "#a5d5fe"
                }
            },
            {
                "scope": [
                    //    "constant.character.escape.c",
                    "constant.other.placeholder.c"
                ],
                "settings": {
                    "foreground": "#ffffff"
                }
            },
            {
                "scope": [
                    "comment.block.c",
                    "comment.line.double-slash.c"
                ],
                "settings": {
                    "foreground": "#698aa7"
                }
            }
        ]
    },
